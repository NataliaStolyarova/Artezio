    "    l = []\n",
    "    for i in range(0, x):\n",
    "        if i % 2 == 0:\n",
    "            continue\n",
    "        else:\n",
    "            l.append(i)\n",
    "            y = i ** 2\n",
    "            print(y)\n",
    "    print(len(l))\n",
    "        \n"
